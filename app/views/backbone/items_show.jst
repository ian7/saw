<table>
<% _(item.attributes).each(function(value, key) { 
	if (key.charAt(0) != '_' && key != "url" ) { %>
<tr>
<td>
	<i><%= key %>: </i>
</td>
<td>
	<span class="edit5" id="<%=key%>"><% if( value ) { %><%= value %><% } else { %>(empty)<% } %></span>
</td>
</tr>

<% }; 
});%>
</table>

